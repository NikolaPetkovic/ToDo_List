<?import javafx.scene.layout.*?>

<HBox maxHeight="110.0" maxWidth="110.0" minHeight="110.0" minWidth="110.0" prefHeight="110.0" prefWidth="110.0" style="-fx-background-color: #1a1a1a;" stylesheets="@../css/DarkTheme.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ArchTaskController">
   <children>
       <VBox fx:id="vBox" maxHeight="100.0" maxWidth="25.0" minHeight="100.0" minWidth="25.0" prefHeight="100.0" prefWidth="25.0" style="-fx-background-color: #333333;" />
      <GridPane maxHeight="100.0" maxWidth="75.0" minHeight="100.0" minWidth="75.0" prefHeight="100.0" prefWidth="75.0" style="-fx-background-color: #333333;">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="5.0" minWidth="5.0" prefWidth="5.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="5.0" minWidth="5.0" prefWidth="5.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="5.0" minHeight="5.0" prefHeight="5.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="17.0" minHeight="17.0" prefHeight="17.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="5.0" minHeight="5.0" prefHeight="5.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="17.0" minHeight="17.0" prefHeight="17.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="17.0" minHeight="17.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="17.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="lbName" prefHeight="17.0" prefWidth="76.0" text="Name" textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="lbDeadline" prefHeight="17.0" prefWidth="76.0" text="Name" textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="lbDone" prefHeight="17.0" prefWidth="76.0" text="Name" textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button alignment="BASELINE_LEFT" mnemonicParsing="false" onAction="#showClickedTask" prefHeight="25.0" prefWidth="60.0" styleClass="btn" text="Info" GridPane.columnIndex="1" GridPane.rowIndex="5">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
            </Button>
         </children>
      </GridPane>
   </children>
